    "\n",
    "# Regroup the data for a complete dataset\n",
    "performance_data = pd.concat([best_performers, \n",
    "                              poorest_performers])\n",
    "\n",
    "# Write to CSV\n",
    "performance_data.to_csv(write_path + \"PERFORMANCE_DATA.csv\", index = False)"
